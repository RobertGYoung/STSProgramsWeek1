import static org.junit.jupiter.api.Assertions.*;

import org.junit.Before;
import org.junit.jupiter.api.Test;
import org.junit.*;

class TestMathimaticalOperations {
	@BeforeClass
	int x,y;
	@Before
	void setup() {
		x=1;
		y=3;	
	}

	
	@Test
	void testAdd() {
		assertTrue(MathimaticalOperations.add(x, y)==4);
		
	}
	@Test 
	void testSubtract() {
		assertTrue(MathimaticalOperations.sub(x, y)==-2);
	}
	@Test
	
}
