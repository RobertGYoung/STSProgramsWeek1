package com.revature.sts05;

public class Person {
	private String name;
	public int age;
	
	public Person() {
		
	}
	
	private String getName() {
		return name;
	}

	
	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}


	public Person(String name, int age) {
		
		name =getName();
		age = getAge();
		
	}
	
}
