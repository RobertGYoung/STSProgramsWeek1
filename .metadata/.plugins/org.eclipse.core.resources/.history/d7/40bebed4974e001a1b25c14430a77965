

package com.revature.day04.comparable;
import java.util.Comparator;

public class PersonComparatorByAge {

	
		@Override
		public int compare(Object person1, Object person2) {
			Integer id1=((ComparablePerson) person1).getId();
			Integer id2=((ComparablePerson) person2).getId();
			return id1.compareTo(id2);
		}
	}
