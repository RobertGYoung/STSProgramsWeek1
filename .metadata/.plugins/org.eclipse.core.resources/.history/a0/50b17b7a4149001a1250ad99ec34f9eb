

public class BubbleSort {
public static void main(String[] args) {
	
	long time1;
	long time2;
	
	int [] arrayToSort = new int[args.length];
	int temp=0;
	System.out.print("[ ");
	for(int i = 0;i<args.length;i++) {
		arrayToSort[i]=Integer.parseInt(args[i]);
		System.out.println(arrayToSort[i]+" ");
	}
	System.out.print(" ]");
	
	//sort
	time1=System.nanoTime();
	System.out.println("\nSORTED ARRAY:");

	for(int j=0;j<arrayToSort.length-1;j++) {
		if(arrayToSort[j]>arrayToSort[j+1]) {
			temp=arrayToSort[j];
			arrayToSort[j]=arrayToSort[j+1];
			arrayToSort[j+1]=temp;
		}
		for(int k=0;k<arrayToSort.length-2;k++) {
		if(arrayToSort[k]>arrayToSort[k+1]) {
			temp=arrayToSort[k];
			arrayToSort[k]=arrayToSort[k+1];
			arrayToSort[k+1]=temp;
		}
		}
		
	}
	System.out.print("[ ");
	for(int i = 0;i<args.length;i++) {
		System.out.println(arrayToSort[i]+" ");
	}
	System.out.print(" ]");
	time2=System.nanoTime();
	
	long timeElapsed= time2-time1;
	long nanoSecondsElapsed = timeElapsed;
	long milliSecondsElapsed = timeElapsed/1000000;
	
	
	System.out.println("Time Elapsed:"+ nanoSecondsElapsed +" or "+milliSecondsElapsed+" milliSeconds");
}


}
