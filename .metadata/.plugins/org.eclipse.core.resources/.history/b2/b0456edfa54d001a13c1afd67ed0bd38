package com.revature.sts05;

public class StringComparison {
	
	public static void main(String[] args) {
		String s1;
		String s2;
		
		s1=new String ("Hello");
		s2=new String ("Hello");
		
		boolean check = false;
		if(s1==s2) {
			check=true;
		}
		
		System.out.println(s1.equals(s2));
		System.out.println(check);
	}

}
